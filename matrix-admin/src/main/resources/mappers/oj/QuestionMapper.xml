<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.matrix.admin.oj.mappers.QuestionMapper">

    <resultMap id="BaseResultMap" type="com.matrix.common.pojo.oj.Question">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="title" column="title" jdbcType="VARCHAR"/>
            <result property="content" column="content" jdbcType="VARCHAR"/>
            <result property="tags" column="tags" jdbcType="OTHER"/>
            <result property="thumbNum" column="thumb_num" jdbcType="INTEGER"/>
            <result property="favourNum" column="favour_num" jdbcType="INTEGER"/>
            <result property="userId" column="user_id" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="isDelete" column="is_delete" jdbcType="SMALLINT"/>
            <result property="answer" column="answer" jdbcType="VARCHAR"/>
            <result property="judgeConfig" column="judge_config" jdbcType="OTHER"/>
            <result property="judgeCase" column="judge_case" jdbcType="OTHER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,title,content,
        tags,thumb_num,favour_num,
        userId,create_time,update_time,
        is_delete,answer,judgeConfig,
        judgeCase
    </sql>
</mapper>
